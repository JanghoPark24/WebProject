<?xml version="1.0" encoding="UTF-8" ?>
 <!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="lectureMap">
	<!-- 중복검사 -->
	
	<select id="getAllLectures" resultType="lectureList">
		select * from lecture
	</select>
	<select id="getSpecificLectures" parameterType="String" resultType="lectureList">
		select * from lecture where major like %#{input}%
	</select>
	


</mapper>








