<?xml version="1.0" encoding="UTF-8" ?>
 <!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="memberns">
	<!-- 중복검사 -->
	<select id="is_dup_email" parameterType="String"
		resultType="member">
		select * from member where upper(email)=#{email}
	</select>

	<!-- 중복검사 -->
	<select id="is_dup_nickname" parameterType="String"
		resultType="member">
		select * from member where upper(nickname)=#{nickname}
	</select>
	
	<!-- 중복검사 -->
	<select id="is_dup_reg_key" parameterType="String"
		resultType="member">
		select * from member where upper(reg_key)=#{reg_key}
	</select>

	<!-- 회원저장 -->
	<insert id="member_join" parameterType="member">
		insert INTO	member	values(	#{email},'서울대학교',#{nickname},'C54F4493021DE0A059CF719A2DCE3D20941EFE9A625122715BADD0B76244F6DD','n',
		'unknown',0,null,'B6E0lnRevAWmKnASHCYY',sysdate,'unknown','unknown.jpg',''
		)
	</insert>


</mapper>







